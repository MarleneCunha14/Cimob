@model IEnumerable<Cimob.Models.Candidatura.Utilizador_Entrevista>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
               Nome Utilizador
            </th>
            <th>
               Data Entrevista
            </th>
            <th>
               Hora Entrevista
            </th>            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.applicationUser.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.entrevista.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.entrevista.Hora)
            </td>            
        </tr>
}
    </tbody>
</table>
</body>
</html>
