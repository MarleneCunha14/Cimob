@model IEnumerable<Cimob.Models.Candidatura.Utilizador_Entrevista>
    <div class="DefaultPresentation">
        <div class="vertical-menu">
            <a href="#" class="active">Gestão de conta</a>
            <a class="SubMenu" asp-area="" asp-controller="Manage" asp-action="ConsultarDados">Consultar Dados</a>
            <a class="SubMenu" asp-area="" asp-controller="Manage" asp-action="AlterarDados">Alterar Dados</a>
            <a class="SubMenu" asp-area="" asp-controller="Manage" asp-action="ChangePassword">Alterar Password</a>
            <a class="SubMenu" asp-area="" asp-controller="Manage" asp-action="VerCandidaturasPendentes">Estado Candidatura</a>
            <a class="SubMenu" asp-area="" asp-controller="Entrevistas" asp-action="IndexUser">Entrevistas Pendentes</a>
        </div>


        <table class="table" style="margin-left:5%">
            <thead>
                <tr>
                    <th>
                        Nome Utilizador
                    </th>
                    <th>
                        Data Entrevista
                    </th>
                    <th>
                        Hora Entrevista
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.applicationUser.Nome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.entrevista.Data)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.entrevista.Hora)
                    </td>
                </tr>
        }
            </tbody>
        </table>
    </div>
