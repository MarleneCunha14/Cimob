@model Cimob.Models.PontosInteresse.PontoInteresse

<div class="DefaultPresentation">
    <div class="vertical-menu">
        <a href="#" class="active">Pontos de Interesse</a>
        <a class="SubMenuSelected" asp-area="" asp-controller="PontoInteresses" asp-action="Index">Consultar Pontos</a>
        <a class="SubMenu" asp-area="" asp-controller="Home" asp-action="Team">Ajuda</a>
    </div>
    <div style="margin-left: 5%;">
        <form asp-action="Index">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <select asp-for="Id" class="form-control" asp-items="ViewBag.Pontos"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Pesquisar Ponto" class="btn btn-default" />
            </div>
        </form>
        <iframe src=@Model.Url width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
    </div>
</div>





